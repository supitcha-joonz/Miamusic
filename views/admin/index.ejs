<%- include('../partials/h-artist.ejs') %>

<section class="py-5 text-center container">
    <div class="row py-lg-5">
      <div class="col-lg-6 col-md-8 mx-auto">
        <h1 class="fw-light">Add Music</h1>
        <p>
          <a href="/admin/addmusic" class="btn btn-primary my-2">AddMusic</a>

        </p>
      </div>
    </div>
</section>

<div class="album py-5">
  <div class="container">
    <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">

    <% music.forEach(function(music){ %>
      <div class="col">
        <div class="card shadow-sm">

          <div class="card-body">
            <h5 class="card-title"><%= music.song %></h5>
            <p class="card-text"><%= music.name %></p>
            
            <div class="d-flex justify-content-between">
              
             
            </div>
          </div>
        </div>
        <br>
      </div>
    <% }); %>
    </div>
  </div>
</div>
  


<%- include('../partials/footer.ejs') %>